#: argument_parser.cpp:63
msgid "\"{}\" not allowed together with named argument \"{}\""
msgstr "\"{}\" დაუშვებელია მითითებულ არგუმენტთან \"{}\" ერთად"

#: argument_parser.cpp:65
msgid "\"{}\" not allowed in command \"{}\""
msgstr "\"{}\" დაუშვებელია ბრძანებაში \"{}\""

#: argument_parser.cpp:67
msgid "\"{}\" not allowed together with positional argument \"{}\""
msgstr "\"{}\" დაუშვებელია პოზიციურ არგუმენტთან \"{}\" ერთად"

#: argument_parser.cpp:77
msgid "Argument id \"{}\" already registered in group \"{}\""
