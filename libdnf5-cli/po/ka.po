msgstr "არასწორი სიმბოლო '.' არგუმენტში id-ით \"{}\""

#: argument_parser.cpp:200 argument_parser.cpp:205
msgid "Too few values for positional argument \"{}\""
msgstr "არასაკმარისი მნიშვნელობა პოზიციური არგუმენტისთვის \"{}\""

#: argument_parser.cpp:245
msgid "Missing value for named argument \"--{}\""
msgstr "აკლია მნიშვნელობა მითითებული არგუმენტისთვის \"--{}\""

#: argument_parser.cpp:253
msgid "Unexpected value for named argumend \"--{}\""
msgstr "მოულოდნელი მნიშვნელობა მითითებული არგუმენტისთვის \"--{}\""

#: argument_parser.cpp:297
msgid "Missing value for named argument \"-{}\""
msgstr "აკლია მნიშვნელობა მითითებული არგუმენტისთვის \"-{}\""

#: argument_parser.cpp:433
msgid "Command id \"{}\" already registered for command \"{}\""
msgstr "ბრძანების ID \"{}\" უკვე რეგისტრირებულია ბრძანებისთვის \"{}\""

#: argument_parser.cpp:444
msgid "Named argument id \"{}\" already registered for command \"{}\""
msgstr "მითითებული არგუმენტის ID \"{}\" უკვე რეგისტრირებულია ბრძანებისთვის \"{}\""

#: argument_parser.cpp:454
