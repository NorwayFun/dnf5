#: argument_parser.cpp:464
msgid "Group id \"{}\" already registered for command \"{}\""
msgstr "ჯგუფის ID \"{}\" უკვე რეგისტრირებულია ბრძანებისთვის \"{}\""

#: argument_parser.cpp:474
msgid "Command id \"{}\" does not contain subcommand with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" ქვებრძანებას ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:482
msgid "Command id \"{}\" does not contain named argument with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" მითითებულ არგუმენტს ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:490
msgid "Command id \"{}\" does not contain positional argument with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" პოზიციურ არგუმენტს ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:497
msgid "Command id \"{}\" does not contain group with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" ჯგუფს ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:589 argument_parser.cpp:712
msgid "Unknown argument \"{}\" for command \"{}\""
msgstr "უცნობი არგუმენტი \"{}\" ბრძანებისთვის \"{}\""

#: argument_parser.cpp:599 argument_parser.cpp:722
msgid "Missing positional argument \"{}\" for command \"{}\""
msgstr "აკლია პოზიციური არგუმენტი \"{}\" ბრძანებისთვის \"{}\""

